<div class="jumbotron">
  <h1 class="display-3">
    Hello,world
  </h1>
</div>
<div class="row">
  <% @post.each do |post| %>
    <div class="col-sm-4">
      <h3><%=post.title%></h3>
      <p><%=truncate(post.content,length: 150)%></p>
      <%=link_to "Read More",post_path(post)%>
    </div>
  <% end %>
</div>
